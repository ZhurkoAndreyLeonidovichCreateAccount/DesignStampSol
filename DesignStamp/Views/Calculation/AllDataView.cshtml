@model StartDetailView

@{
    ViewData["Title"] = "AllDataView";
}


<table class="table table-striped">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Length)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Width)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Thickness)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShearingStress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AmountDiametrHoles)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.StampName)
        </th>

    </tr>



    <tr>
        <td>
            @Html.DisplayFor(modelItem => modelItem.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => modelItem.Length)
        </td>
        <td>
            @Html.DisplayFor(modelItem => modelItem.Width)
        </td>
        <td>
            @Html.DisplayFor(modelItem => modelItem.Thickness)
        </td>
        <td>
            @Html.DisplayFor(modelItem => modelItem.ShearingStress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => modelItem.AmountDiametrHoles)
        </td>

        <td>
            @Html.DisplayFor(modelItem => modelItem.StampName)
        </td>


    </tr>

</table>
        @if (Model.AmountDiametrHoles!=0)
        {
            <br />
            <partial name="_DifferHolePartial" model="Model.DifferHoles" />
            <br />
        }



<div class="row">
    <a asp-action="StartCalculation" class="col-md-4 btn btn-secondary">Начать расчет</a>
    <a class="col-md-4 offset-md-4 btn btn-secondary" asp-action="DifferHoles" asp-route-flag="1">Назад</a>
   
</div>
